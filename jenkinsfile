pipeline {
    agent any

    stages {
        stage('Call this job') {
            steps {
                sh 'echo "First_job"'
                sh 'echo "second_job.."'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing.."'
            }
        
        }
        stage('Call this job') {
            steps {
                sh 'sudo python3 /root/tt.py'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
